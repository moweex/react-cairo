{
  "cli": {
    "version": ">= 3.18.3"
  },
  "build": {
    "development": {
      "channel": "development",
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_API_URL": "yourhost.com",
        "EXPO_PUBLIC_EVENT_ID": "supabase_manually_created_event_id"
      }
    },
    "preview": {
      "channel": "preview",
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_API_URL": "yourhost.com",
        "EXPO_PUBLIC_EVENT_ID": "supabase_manually_created_event_id"
      }
    },
    "production": {
      "channel": "production",
      "env": {
        "EXPO_PUBLIC_API_URL": "yourhost.com",
        "EXPO_PUBLIC_EVENT_ID": "supabase_manually_created_event_id"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
